while wait() do
local args = {
    [1] = "Punching",
    [2] = workspace.PowerTrainingFolder.Model
}

game:GetService("ReplicatedStorage").Network.Port1:FireServer(unpack(args))
local args = {
    [1] = "Vitality"
}

